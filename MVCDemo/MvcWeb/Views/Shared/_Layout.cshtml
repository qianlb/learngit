    <script>
        $(function () {
            function leftbarPatch() {
                $(".leftScrollCon").css("height", (window.innerHeight - $(".logo-env")[0].offsetHeight - $("#menuTitle")[0].offsetHeight) + "px");
                $('[data-sidebar]').css('height', ($(window).height() - 50) + 'px');
            }

            $(window).resize(function () {
                leftbarPatch();
            });

            $(".sidebar-menu").unbind("DOMSubtreeModified").bind("DOMSubtreeModified", function () {
                leftbarPatch();
            });

            leftbarPatch();
            $('.overlay-menu').slimscroll({
                height: '100%',
                wheelStep: 35
            });

            $('#sidebar-bundle > li > a').unbind('click').bind('click', function () {
                var _this = $(this);
                var id = _this.data("id");
                $(".dropdown-div").addClass("hidden");
                $("#" + id + "").removeClass("hidden");
                var data = id === 'country-latin' ? 'Latam' : 'China';
                $.ajax({
                    url: '/Home/SetUserBundle',
                    type: 'get',
                    dataType: 'json',
                    data: { bundle: data },
                    axync: true,
                    success: function () {
                        $(".btn-group > .dropdown-toggle > .sidebar-name").text(_this.text());
                        $(".btn-group > .dropdown-toggle").trigger('click');
                        if ($('.bunlde-ul').length > 1) {
                            if (id === 'country-latin') {
                                $('#bundle-latam').removeClass('hidden').siblings().addClass('hidden');
                            } else {
                                $('#bundle-china').removeClass('hidden').siblings().addClass('hidden');
                            }
                        }
                    }
                });
            });
            if (!$("#errorPage").val()) {
                /****User Terms***/
                if ($("#UserTerms").data("check").toLowerCase() === 'false') {

                    $('#UserTerms').on('click', '#agree-checkbox', function () {
                        var _this = $(this);
                        if (_this.hasClass('checked')) {
                            _this.removeClass('checked');
                            $('#submit-btn').addClass('disabled btn-default').removeClass('btn-primary');
                        } else {
                            _this.addClass('checked');
                            $('#submit-btn').removeClass('disabled btn-default').addClass('btn-primary');
                        }
                    });
                    $('#UserTerms').modal({
                        keyboard: false,
                        show: true,
                        backdrop: 'static'
                    });
                    $('#UserTerms').on('shown.bs.modal', function () {

                        $.ajax({
                            url: '/Home/GetUserTermsContent',
                            type: 'get',
                            dataType: 'html',
                            axync: true,
                            success: function (result) {
                                $('#UserTerms .modal-content').html(result);
                                $('#UserTerms .tag-primary:odd').addClass('col-xs-offset-1');
                                $('#text-scroll').height(88).perfectScrollbar({
                                    minScrollbarLength: 10
                                });
                                $('.ps-scrollbar-y-rail').css({ 'display': 'block', 'opacity': 1 });
                            }
                        });
                    });

                    $('#UserTerms').on('click', '#submit-btn', function () {
                        $('#UserTerms').modal('hide');

                        $.ajax({
                            url: '/Account/UpdateUserTerms',
                            type: "post",
                            async: true,
                            dateType: "json",
                            success: function () {

                            }
                        });
                    });
                }
            }

        });
    </script>
